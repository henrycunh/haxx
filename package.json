  "description": "a very opinionated runtime for scripts, it simply works",
  "packageManager": "pnpm@8.1.1",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "haxx": "./dist/haxx.mjs"
  },
